$(".load-more").remove();
$(".table-leaderboard").append("<%= j render partial: 'leaderboards', locals: { leaderboards: @leaderboards } %>");
$("#more-anchor").after("<%= j render partial: 'shared/load_more', locals: { collection: @leaderboards } %>");
$('html, body').animate({
   scrollTop: $(document).height()-$(window).height()},
   1400,
   "easeOutQuint"
);
