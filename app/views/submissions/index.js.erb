$(".load-more").remove();
$("#submissions-div").append("<%= j render partial: 'submissions', locals: { submissions: @submissions } %>");
$("#more-anchor").after("<%= j render partial: 'shared/load_more', locals: { collection: @submissions } %>");
$('html, body').animate({
   scrollTop: $(document).height()-$(window).height()},
   1400,
   "easeOutQuint"
);
